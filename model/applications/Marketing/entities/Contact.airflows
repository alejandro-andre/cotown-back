        label en_US: "Phone"
        length: 100
        list
        order: 4
        sm: 6
        textFilter
        type: TEXT
        visible
        xs: 12
    }
    attribute Message {
        basicFilter
        label es_ES: "Mensaje"
        label en_US: "Message"
        length: 4000
        multiline
        order: 5
        sm: 12
        textFilter
        type: TEXT
        visible
        xs: 12
